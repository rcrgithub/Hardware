<h1>Inicia sesion</h1>

 <%= form_tag sign_up_path do %>
  <p>
    <%= label_tag :email %><br />
    <%= text_field_tag :email, params[:email] %>
  </p>
  <p>
    <%= label_tag :password %><br />
    <%= password_field_tag :password %>
  </p>
  <p class="button"><%= submit_tag "Iniciar sesion" %></p>
 <% end %>
